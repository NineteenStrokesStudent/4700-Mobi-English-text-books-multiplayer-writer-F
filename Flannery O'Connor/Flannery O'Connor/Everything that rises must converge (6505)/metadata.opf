        <dc:title>Everything that rises must converge</dc:title>
        <dc:creator opf:file-as="O'Connor, Flannery" opf:role="aut">Flannery O'Connor</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
